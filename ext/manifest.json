{
  "name": "Add Swagger to Eureka",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "A Chrome Extension to add Swagger shortcut in Eureka.",
  "homepage_url": "https://github.com/Rikezenho",
  "author": "Henrique Kuwai",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "pt",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Add Swagger to Eureka Extension"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["src/inject/eureka.js"]
    }
  ],
  "permissions": [
    "https://*/*, http://*/*"
  ]
}
